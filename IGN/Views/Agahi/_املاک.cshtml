@using IGN.Models;
<!----------شروع املاک---------->



@{
    if (ViewBag.lv1 == "املاک" && ViewBag.lv2 == "فروش-املاک-مسکونی" && ViewBag.lv3 == null)
    {
               <!----------شروع فروش املاک مسکونی---------->

        <div class="row">
            <div class="col-md-2">
                <div class="field-container radio-field">
                    <label>حومه شهر</label>
                    <div class="md-radio md-radio-inline">
                        <input id="Side-All" type="radio" value="0" name="Countryside" checked>
                        <label for="Side-All">فرقی نمی کند</label>
                    </div>
                    <div class="md-radio md-radio-inline">
                        <input id="Side-No" type="radio" value="1" name="Countryside">
                        <label for="Side-No">نیست</label>
                    </div>
                    <div class="md-radio md-radio-inline">
                        <input id="Side-Yes" type="radio" value="2" name="Countryside">
                        <label for="Side-Yes">هست</label>
                    </div>
                </div>
            </div>
            <div class="col-md-2">
                <div class="field-container radio-field">
                    <label>نوع آگهی دهنده</label>
                    <div class="md-radio md-radio-inline">
                        <input id="Person-All" value="0" type="radio" name="Person" checked>
                        <label for="Person-All">همه</label>
                    </div>
                    <div class="md-radio md-radio-inline">
                        <input id="Person-Self" value="1" type="radio" name="Person">
                        <label for="Person-Self">شخصی</label>
                    </div>
                    <div class="md-radio md-radio-inline">
                        <input id="Person-Prop" value="2" type="radio" name="Person">
                        <label for="Person-Prop">مشاوراملاک</label>
                    </div>
                </div>
            </div>
            <div class="col-md-2">
                <div class="field-container radio-field">
                    <label>نوع</label>
                    <div class="md-radio md-radio-inline">
                        <input id="Kind-All" type="radio" value="0" name="Kind" checked>
                        <label for="Kind-All">فرقی نمی کند</label>
                    </div>
                    <div class="md-radio md-radio-inline">
                        <input id="Kind-Sell" type="radio" value="1" name="Kind">
                        <label for="Kind-Sell">فروشی</label>
                    </div>
                    <div class="md-radio md-radio-inline">
                        <input id="Kind-Need" type="radio" value="2" name="Kind">
                        <label for="Kind-Need">درخواستی</label>
                    </div>
                </div>
            </div>

            <div class="col-md-3">

                <div class="field-container int-field">
                    <label>قیمت کل</label>
                    <div class="row">
                        <div class="col-md-12 no-padding">
                            <select id="Kind-TotalPrice" data-placeholder="مقطوع- تومان" class="select width-100 SelectStyle" >
                                 
                                <option value="1">مقطوع - تومان</option>
                                <option value="1">مجانی</option>
                                <option value="1">توافقی</option>
                                <option value="1">جهت معاوضه</option>
                            </select>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6 no-padding">
                            <input id="Untill-TotalPrice"  class="form-control keyword numbertxt float-left" name="keyword" value="" placeholder="تا" type="text">
                        </div>
                        <div class="col-md-6 no-padding">
                            <input id="From-TotalPrice" class="form-control keyword numbertxt" name="keyword" value="" placeholder="از" type="text">
                        </div>
                    </div>
                </div>

            </div>



            <div class="col-md-3">

                <div class="field-container int-field">
                    <label>متراژ (متر مربع)</label>
                    <div class="row">
                        <div class="col-md-6 no-padding">
                            <input id="Untill-Area"  class="form-control keyword numbertxt float-left" name="keyword" value="" placeholder="تا" type="text">
                        </div>
                        <div class="col-md-6 no-padding">
                            <input id="From-Area" class="form-control keyword numbertxt" name="keyword" value="" placeholder="از" type="text">
                        </div>
                    </div>
                </div>

            </div>



        </div>
        <div class="row">
            <div class="col-md-8 search-col">
                <button id="SearchSaleHome" class="btn btn-common btn-search btn-block search-btn"><strong>جستجو</strong></button>
            </div>
            <div class="col-md-2">
                <div class="row margin-top-20">
                    <div class="col-md-6">
                        <div class="cntr">
                            <div class="row ">
                                <input type="checkbox" id="HasFast" class="cbx hidden" />
                                <label for="HasFast" class="lbl"></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 text-align-left">
                        <span>فوری</span>
                    </div>
                </div>
            </div>
            <div class="col-md-2">
                <div class="row margin-top-20">
                    <div class="col-md-6">
                        <div class="cntr">
                            <div class="row ">
                                <input type="checkbox" id="HasImage" class="cbx hidden" />
                                <label for="HasImage" class="lbl"></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 text-align-left">
                        <span>عکس دار</span>
                    </div>
                </div>
            </div>
        </div>
    }
}







