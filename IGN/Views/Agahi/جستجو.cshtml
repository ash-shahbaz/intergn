    <script>

        $("#mahale").chosen();
        $('ShahrSelect').chosen();

        $(".CitySelect").click(function () {

            alert("HI");


            $.ajax({
                type: "GET",
                url: "/Sync/GetRegionByCityID/" + $(this).val() + "",
                success: function (data) {

                    var MyData = $.parseJSON(data);


                    if (MyData.length == 0) {
                        $("#mahale").chosen();
                        $("#mahale").empty();

                    }
                    else {

                        for (var i = 0; i < MyData.length; i++) {
                            var $li = "<option value=\" " + MyData[i].RegionID + "\" class=\"\">" + MyData[i].RegionName + "</option>";
                            $("#mahale").append($li);
                        }
                        $("#mahale").chosen();

                    }

                }, error: function () {
                    $("#mahale").empty();
                }
            })
        });




        var arr;
        $("#RetToLoadCate").click(function () {
            $(".Level1").empty();

            $("#CatFlow").empty();
            LoadCategory();


        })










        LoadCategory();


        function LoadCategory() {

            $.ajax({
