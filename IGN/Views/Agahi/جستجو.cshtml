    ViewBag.Title = "جستجو_آگهی";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Head
{
    <link href="~/css/animate.min.css" rel="stylesheet" />

    @*<link rel="stylesheet" href="~/css/main.css">*@
    <link rel="stylesheet" href="~/css/slidr.css">

