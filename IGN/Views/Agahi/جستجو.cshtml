                            <option value="3">Value 3</option>
                            <option value="4">Value 4</option>
                        </select>
                        <br />
                    </div>

                </div>

                <div class="col-md-3  search-col">
                    <span class=" user-type">
                        <button type="button" class="btn btn-info btn-lg btnchoose" data-toggle="modal" data-target="#myModal">انتخاب گروه آگهی</button>

                        <!-- Modal -->
                        <div class="modal fade" id="myModal" role="dialog">
                            <div class="modal-dialog whitebg">

                                <!-- Modal content-->
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                                        <h4 class="modal-title">انتخاب گروه بندی</h4>
                                        <h5> <a href="#">دسته بندی :</a>  <span id="RetToLoadCate"> بازگشت </span></h5>
                                        <br />
                                        <span id="CatFlow"></span>
                                    </div>
                                    <div class="modal-body choose ">
                                        <ul class="Level1"></ul>
                                    </div>

                                </div>

                            </div>
                        </div>

                    </span>

                </div>


                <div class="col-md-3 search-col">
                    <input class="form-control keyword" name="keyword" value="" placeholder="متن جستجو" type="text">
                    <i class="fa fa-search"></i>

                </div>
            </div>

            @{ if (ViewBag.Name != true)
                {

                    @*------شروع  صفحه اصلی جستجو----------*@
                    <div class="row">
                        <div class="col-md-12">
                            <div class="field-container radio-field">
                                <label>نوع</label>
                                <div class="md-radio md-radio-inline">
                                    <input id="4" type="radio" name="g3" checked>
                                    <label for="4">فرقی نمی کند</label>
                                </div>
                                <div class="md-radio md-radio-inline">
                                    <input id="5" type="radio" name="g3" checked>
                                    <label for="5">فروشی</label>
                                </div>
                                <div class="md-radio md-radio-inline">
                                    <input id="6" type="radio" name="g3">
                                    <label for="6">درخواستی</label>
                                </div>
                            </div>
                        </div>
                    </div>
                    @*------پایان صفحه اصلی جستجو----------*@
                }
            }








            @{

                string l1 = ViewBag.Level1;
                string l2 = ViewBag.Level2;
                string l3 = ViewBag.Level3;


                ViewBag.lv1 = l1;
                ViewBag.lv2 = l2;
                ViewBag.lv3 = l3;




                switch (l1)
                {
                    case "املاک":
                        {


                            Html.RenderPartial("_املاک");
                            break;
                        }
                    case "لوازم-الکترونیکی":
                        {

                            Html.RenderPartial("_لوازم_الکترونیک");
                            break;
                        }
                    case "وسایل-نقلیه":
                        {
                            Html.RenderPartial("_وسایل_نقلیه");
                            break;
                        }
                    case "لوازم-خانگی":
                        {

                            Html.RenderPartial("_لوازم_کسب_سرگرمی");
                            break;
                        }
                    case "لوازم-شخصی":
                        {
                            Html.RenderPartial("_لوازم_کسب_سرگرمی");
                            break;
                        }
                    case "سرگرمی-و-فراغت":
                        {
                            Html.RenderPartial("_لوازم_کسب_سرگرمی");
                            break;
                        }
                    case "کسب-و-کار":
                        {
                            Html.RenderPartial("_استخدام_اجتماعی_خدمات");
                            break;
                        }

                    case "خدمات":
                        {
                            Html.RenderPartial("_استخدام_اجتماعی_خدمات");
                            break;
                        }


                    case "اجتماعی":
                        {
                            Html.RenderPartial("_استخدام_اجتماعی_خدمات");
                            break;
                        }

                    case "a":
                        {

                            break;
                        }
                }
                Html.RenderPartial("_مشترک");


            }




