                        </div>
                    </div>
                </div>
                    @*------پایان صفحه اصلی جستجو----------*@

            string l1 = ViewBag.Level1;
            string l2 = ViewBag.Level2;
            string l3 = ViewBag.Level3;


            ViewBag.lv1 = l1;
            ViewBag.lv2 = l2;
            ViewBag.lv3 = l3;

            switch (l1)
            {
                case "املاک":
                    {

                        Html.RenderPartial("_املاک");
                        break;
                    }
                case "لوازم-الکترونیکی":
                    {

                        Html.RenderPartial("_لوازم_الکترونیکی");
                        break;
                    }
                case "وسایل-نقلیه":
                    {
                        Html.RenderPartial("_وسایل_نقلیه");
                        break;
                    }
                case "لوازم-خانگی":
                    {

                        Html.RenderPartial("_لوازم_کسب_سرگرمی");
                        break;
                    }
                case "لوازم-شخصی":
                    {
                        Html.RenderPartial("_لوازم_کسب_سرگرمی");
                        break;
                    }
                case "سرگرمی-و-فراغت":
                    {
                        Html.RenderPartial("_لوازم_کسب_سرگرمی");
                        break;
                    }
                case "کسب-و-کار":
                    {
                        Html.RenderPartial("_استخدام_اجتماعی_خدمات");
                        break;
                    }

                case "خدمات":
                    {
                        Html.RenderPartial("_استخدام_اجتماعی_خدمات");
                        break;
                    }


                case "اجتماعی":
                    {
                        Html.RenderPartial("_استخدام_اجتماعی_خدمات");
                        break;
                    }

                case "a":
                    {

                        break;
                    }
            }




            Html.RenderPartial("_مشترک");


            }




