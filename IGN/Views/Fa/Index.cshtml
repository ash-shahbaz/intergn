                <main class="maintabs">

                    <input id="tab1" type="radio" class="display-none" name="tabs" checked>
                    <label class="lbltabs" for="tab1">روز</label>

                    <input id="tab2" type="radio" class="display-none" name="tabs">
                    <label class="lbltabs" for="tab2">هفته</label>

                    <input id="tab3" type="radio" class="display-none" name="tabs">
                    <label class="lbltabs" for="tab3">ماه</label>

                    <input id="tab4" type="radio" class="display-none" name="tabs">
                    <label class="lbltabs" for="tab4">سال</label>

                    <section class="sectiontabs" id="content1">


                        <div class="widget widget-news">

                            <div class="widget-content">
                                <ul class="list-unstyled" id="DayTagNumber">

                                    @{
                                        foreach (var item in Utility.lstTopTagDay)
                                        {
                                            var url = "/fa/tags/" + item.HashtagName;
                                            <li><div class="item clearfix"><div class="row"><div class="col-md-3 col-xs-3 col-sm-3 no-padding"><ul class="SourceNews"><li>@item.repcount  خبر</li></ul></div><div class="col-md-9 col-xs-9 col-sm-9 no-padding"> <div><div class="boxin-right"><span class="fa fa-angle-double-left HomeBoxes" aria-hidden="true"></span></div><h1 class="titleNews"><a href="@url">@item.HashtagName</a></h1></div></div></div></div></li>


                                        }
                                    }



                                </ul>
                            </div>

                        </div>



                    </section>

