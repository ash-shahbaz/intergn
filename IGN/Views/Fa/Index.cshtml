    <script>
        function SetAndBindNewsBoxByCateIDAndResult(CategoryID, result) {

            var foundSports = $.grep(result, function (v) {
                return v.CategoryID === CategoryID;
            });

            $.each(foundSports, function (i, item) {

                var splitTitle = item.Title.replace("+", " ").replace("?", " ").replace("*", " ").replace(";", " ").replace(",", " ").replace(".", " ").replace(":", " ").replace("؛", " ").replace("؟", " ").replace("»", " ").replace("«", " ").replace("!", " ").replace("(", " ").replace(")", " ").replace("{", " ").replace("}", " ").replace("[", " ").replace("]", " ").replace("/", " ").replace("-", " ");
                var SeoUrl = splitTitle.split(" ");
                var url = "";
                for (var i = 0; i < SeoUrl.length; i++) {

                    if (url.length > 80) {
                        break;
                    } else {
                        url += SeoUrl[i] + '-';
                    }
                }
                //+ item.rssname +
                var $qli = "<li><div class=\"item clearfix\" ><div class=\"row\"><div class=\"col-md-3 col-xs-3 col-sm-3 no-padding\"><ul class=\"SourceNews\"><li>" + "چند دقیقه قبل" + "</li></ul></div><div class=\"col-md-9 col-xs-9 col-sm-9  no-padding\"><div><div class=\"boxin-right\"><span class=\"fa fa-angle-double-left HomeBoxes\" aria-hidden=\"true\"></span></div><h1 class=\"titleNews\"><a href=\"/fa/News/Sport/" + item.NewsID + "/" + url + "\" target=\"_blank\">" + item.Title + "</a></h1></div></div></div></div></li>";

                $("#News" + CategoryID + "").append($qli);

            });

        }

        $(document).ready(function () {



            GetAllNewsTop10();

